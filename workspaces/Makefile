all:
	$(MAKE) -C applications
	$(MAKE) -C host
	$(MAKE) -C linux-runtime
	$(MAKE) -C linux-host
	$(MAKE) -C nitro-runtime
	$(MAKE) -C nitro-host

clean:
	$(MAKE) -C applications clean
	$(MAKE) -C host clean
	$(MAKE) -C linux-runtime clean
	$(MAKE) -C linux-host clean
	$(MAKE) -C nitro-runtime clean
	$(MAKE) -C nitro-host clean

